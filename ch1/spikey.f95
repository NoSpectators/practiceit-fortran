! Write a complete program in a class named Spikey that prints the 
! following output:
!   \/
!  \\//
! \\\///
! ///\\\
!  //\\
!   /\

program Spikey
    implicit none
    write(*,*) "  \/  "
    write(*,*) " \\// "
    write(*,*) "\\\///"
    write(*,*) "///\\\"
    write(*,*) " //\\ "
    write(*,*) "  /\  "
end program Spikey
